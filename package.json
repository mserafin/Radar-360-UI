{
  "name": "radar-ui",
  "version": "1.0.0",
  "description": "Radar 360 UI",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "lint:fix": "./node_modules/.bin/eslint . --fix",
    "install:radar-server": "cd server && yarn install --pure-lockfile",
    "build:dev": "webpack --bail --config webpack.config.js",
    "build:local": "yarn install --pure-lockfile && npm run install:radar-server && npm run build:dev",
    "start:dev-server": "webpack-dev-server --port 8080 --host 0.0.0.0 --hot --inline --config ./webpack.config.js --content-base ./dist",
    "start:radar-server": "node server/index.js 8090 COM6 115200",
    "start": "npm-run-all --parallel start:radar-server start:dev-server"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "css-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "npm-run-all": "^4.1.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "eslint": "^5.1.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0"
  },
  "dependencies": {
    "p5": "^0.7.2",
    "socket.io-client": "^2.1.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mserafin/Radar-360-UI"
  },
  "author": "Mariusz Serafin",
  "contributors": [
    {
      "name": "charconstpointer",
      "email": "attwistingnether@gmail.com"
    }
  ],
  "license": "MIT"
}
